{
    "name": "ducafecat-vue-frontend-node-server",
    "script": "./src/index.js",
    "cwd": "./",
    "watch": [
        "public",
        "src"
    ],
    "ignore_watch": [
        "node_modules",
        "logs"
    ],
    "watch_options": {
        "followSymlinks": false
    },
    "error_file": "./logs/app-err.log",
    "out_file": "./logs/app-out.log",
    "env": {
        "NODE_ENV": "production"
    }
}